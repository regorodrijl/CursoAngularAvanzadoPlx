{
  "name": "angular-blocks",
  "description": "Tutorial Angular Avanzado Plexus",
  "version": "7.1.0",
  "scripts": {
    "build:client-and-server-bundles": "ng build --prod && ng run angular-blocks:server:production",
    "build:math": "ng build math",
    "build:pwa": "ng build --prod --base-href / ",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build": "ng build --prod --base-href /dist/angular-blocks/ ",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "ng": "ng",
    "pack:math": "npm run build:math && cd dist/math && npm pack",
    "serve:ssr": "node dist/server",
    "start:es": "ng serve --configuration=es -o --port 4073",
    "start:pwa": "npm run build:pwa && http-server ./dist/angular-blocks/ -c-1 -p4072 -a localhost -o",
    "start:ssr": "npm run build:ssr && npm run serve:ssr",
    "start": "ng serve -o --port 4071",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.1.3",
    "@angular/common": "~7.1.3",
    "@angular/compiler": "~7.1.3",
    "@angular/core": "~7.1.3",
    "@angular/forms": "~7.1.3",
    "@angular/http": "~7.1.3",
    "@angular/platform-browser": "~7.1.3",
    "@angular/platform-browser-dynamic": "~7.1.3",
    "@angular/platform-server": "~7.1.3",
    "@angular/pwa": "^0.11.1",
    "@angular/router": "~7.1.3",
    "@angular/service-worker": "~7.1.3",
    "@ngrx/effects": "^6.1.2",
    "@ngrx/store": "^6.1.2",
    "@ngrx/store-devtools": "^6.1.2",
    "@nguniversal/express-engine": "^7.0.2",
    "@nguniversal/module-map-ngfactory-loader": "v7.0.2",
    "core-js": "^2.5.4",
    "express": "^4.15.2",
    "mini.css": "^3.0.1",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular-devkit/build-ng-packagr": "~0.11.0",
    "@angular/cli": "~7.1.2",
    "@angular/compiler-cli": "~7.1.3",
    "@angular/language-service": "~7.1.3",
    "@types/express": "^4.16.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "protractor": "~5.4.0",
    "ts-loader": "^5.2.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6",
    "webpack-cli": "^3.1.0"
  }
}
