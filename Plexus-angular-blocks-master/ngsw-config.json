{
  "index": "/index.html",
  "appData": {
    "versionMessage": "New version: More features to come!"
  },
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/common*.js",
          "/main*.js",
          "/ngsw*.js",
          "/woker*.js",
          "/polyfills*.js",
          "/runtime*.js"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/icons/**",
          "/*.js"
        ]
      }
    }
  ],
  "dataGroups": [{
    "name": "cache-first",
    "urls":[
      "http://127.0.0.1:8080/assets/blocks.json"
    ],
    "cacheConfig":{
      "strategy":"performance",
      "maxSize":10,
      "maxAge":"1d"
    }
  }
  ,{
    "name": "api-first",
    "urls":[
      "https://api-base.herokuapp.com/api/pub/items/**"
    ],
    "cacheConfig":{
      "strategy" : "freshness",
      "maxSize": 100,
      "maxAge" : "1h",
      "timeout" : "5s"
    }
}

]
}
